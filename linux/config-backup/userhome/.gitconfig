[user]
	name = #your name
	email = #your email
[credential]
	helper = store
[core]
	editor = nano
	quotepath = false
[color]
	ui = true
[format]
	pretty = oneline
[merge]
	tool = diffuse
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[alias]
